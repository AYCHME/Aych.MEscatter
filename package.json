{
  "name": "ual-scatter",
  "version": "0.1.8",
  "main": "dist/index.js",
  "license": "MIT",
  "author": {
    "name": "Block.one",
    "url": "https://block.one"
  },
  "contributors": [
    "Nasser Abouelazm",
    "Chris Allnutt",
    "Randy Torres",
    "Jeffrey Smith II"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "clean-build": "rm -rf dist && yarn build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "prepublish": "yarn clean-build",
    "test": "jest"
  },
  "dependencies": {
    "@scatterjs/core": "2.7.51",
    "@scatterjs/eosjs2": "1.5.33",
    "elliptic": "6.5.2",
    "eosjs": "21.0.1-rc1",
    "universal-authenticator-library": "0.1.4"
  },
  "resolutions": {
    "cache-base": "4.0.0"
  },
  "jest": {
    "verbose": false,
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts?)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/dist"
    ],
    "testRegex": "(/src/.*(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "jsdom"
  },
  "devDependencies": {
    "@babel/runtime": "7.2.0",
    "@blockone/eslint-config-blockone": "3.0.0",
    "@types/elliptic": "6.4.10",
    "@types/jest": "24.0.15",
    "@types/node": "12.12.20",
    "eslint": "6.8.0",
    "jest": "24.9.0",
    "ts-jest": "24.0.2",
    "typescript": "3.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EOSIO/ual-scatter"
  }
}
